<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="441.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gps.GPSController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#loadTrack" text="Load" />
                  <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Graph">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#showPlotter" text="2D Plot" />
                  <MenuItem mnemonicParsing="false" onAction="#showElevationGainVsTime" text="Elevation Gain Vs Time" />
                  <MenuItem mnemonicParsing="false" onAction="#showSpeedPlot" text="Speed vs Path" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
      <Label layoutX="14.0" layoutY="38.0" prefHeight="18.0" prefWidth="79.0" text="Track Selected:" />
      <Spinner fx:id="trackSpinner" layoutX="105.0" layoutY="34.0" onMouseClicked="#changeTrackStatDisplay" prefHeight="26.0" prefWidth="289.0" />
      <TextField fx:id="maxLat" editable="false" layoutX="168.0" layoutY="111.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="minLat" editable="false" layoutX="319.0" layoutY="111.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="maxElevMet" editable="false" layoutX="168.0" layoutY="199.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="minElevMet" editable="false" layoutX="319.0" layoutY="199.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="dMiles" editable="false" layoutX="168.0" layoutY="288.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="dKilometers" editable="false" layoutX="319.0" layoutY="288.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="avSpeedKPH" editable="false" layoutX="319.0" layoutY="342.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="avSpeedMPH" editable="false" layoutX="168.0" layoutY="342.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="maxSpeedMPH" editable="false" layoutX="168.0" layoutY="390.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="maxSpeedKPH" editable="false" layoutX="319.0" layoutY="390.0" prefHeight="26.0" prefWidth="94.0" />
      <Label layoutX="204.0" layoutY="90.0" text="Max" />
      <Label layoutX="356.0" layoutY="90.0" text="Min" />
      <TextField fx:id="minLong" editable="false" layoutX="319.0" layoutY="150.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="maxLong" editable="false" layoutX="168.0" layoutY="150.0" prefHeight="26.0" prefWidth="94.0" />
      <Label layoutX="110.0" layoutY="115.0" text="Latitude" />
      <Label layoutX="105.0" layoutY="154.0" text="Longitude" />
      <Label layoutX="108.0" layoutY="217.0" text="Elevation" />
      <Label layoutX="201.0" layoutY="267.0" text="Miles" />
      <Label layoutX="338.0" layoutY="267.0" text="Kilometers" />
      <Label layoutX="84.0" layoutY="293.0" text="Total Distance" />
      <Label layoutX="202.0" layoutY="321.0" text="MPH" />
      <Label layoutX="355.0" layoutY="321.0" text="KPH" />
      <Label layoutX="81.0" layoutY="346.0" text="Average Speed" />
      <Label layoutX="76.0" layoutY="394.0" text="Maximum Speed" />
      <Label layoutX="429.0" layoutY="39.0" text="Tracks Remaining:" />
      <TextField fx:id="tracksRemainingBox" editable="false" layoutX="532.0" layoutY="35.0" prefHeight="25.0" prefWidth="54.0" text="10" />
      <TextField fx:id="maxElevFeet" editable="false" layoutX="168.0" layoutY="229.0" prefHeight="26.0" prefWidth="94.0" />
      <TextField fx:id="minElevFeet" editable="false" layoutX="319.0" layoutY="229.0" prefHeight="26.0" prefWidth="94.0" />
      <Label layoutX="429.0" layoutY="204.0" text="Meters" />
      <Label layoutX="429.0" layoutY="234.0" text="Feet" />
   </children>
</AnchorPane>
